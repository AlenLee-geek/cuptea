page, .container{
  width:100%;
  
}

.navigator{
  display: flex;
  flex-direction: row;
  position: fixed;
  top:0;
  left: 0;
  z-index: 999;
  height: 44px;
  width: 100%;
  background-color: rgba(3,169,244,0);
  padding-top: 20px;
 
  
  
}
.center{
  display: flex;
  justify-content: center;
  align-items: center;
  width: 50px;
  height: 44px;
}
.center > image{
  width: 25px;
  height: 25px;
}
.title{
  display: flex;
  justify-content: center;
  align-items: center;
  flex: 1;
  height: 44px;
  margin-right: 45px;
 
}
.title > text{
  font-size: 16px;
  font-weight: 500;
}

.slide{
   box-shadow: 0px 5px 20px -5px #ddd;
}

.slide-image{
  width: 100%;
  height: 400px;
 
}


.container{
  
   display: flex;
   justify-content: center;
   align-items: center;
   background-color: #fff;
   border-radius: 10px;
   width: 96%;
   min-height: 100px;
   
   margin: 0 auto;
}
.container-title{
  display: flex;
  justify-content: center;
  align-items: center;
  border-radius: 10px 10px 0px 0px;
  border-bottom: 2px dashed #eee;
  background-color: #fff;
  width: 100%;
  height: 45px;


}
.container-title > text{
  font-size: 16px;
  font-weight: 300;
  color: #000;
}

.container-detail{
  display: flex;
  justify-content: center;
  align-items: center;
  padding: 10px;
  box-shadow: 0px 5px 20px -5px #ddd;
}
.container-text{
  display: flex;
  justify-content: center;
  align-content: center;
  
  padding: 10px;

}
.container-text > text{
  font-size: 13px;
  font-weight: 300;
}

.action{
  position: fixed;
  display: flex;
  justify-content: center;
  align-items: center;
  bottom: 5px;
  left: 8px;
  width: 94%;
  height: 50px;
  border: 1px solid #eee;
  background-color: #fff;
  
  border-radius:22px;
  box-shadow: 1px 0px 9px #ddd; 
  

}

.cell-icon{
  display: flex;
  justify-content: center;
  align-items: center;
  flex: 1;
  flex-direction: column;
}

.cell-icon-img{
  display: flex;
  justify-content: center;
  align-items: center;

}
.cell-icon-img > image{
  height: 30px;
  width: 30px;
}

.cell-icon-text{
  display: flex;
  justify-content: center;
  align-items: center;
}

.cell-icon-text > text{
  font-size: 12px;
  font-weight: 300;
}



